{
    "include": [
        "src"
    ],
    "exclude": [
        "doc"
    ],
    "typingsPath": "src/typestubs",
    "reportTypeshedErrors": true,
    "reportMissingImports": false,
    "pythonVersion": "3.7",
    "pythonPlatform": "Darwin",
    "executionEnvironments": [{
            "root": "src/web",
            "pythonVersion": "3.5",
            "pythonPlatform": "Windows",
            "extraPaths": [
                "src/service_libs"
            ]
        },
        {
            "root": "src/sdk",
            "pythonVersion": "3.0",
            "extraPaths": [
                "src/backend"
            ]
        },
        {
            "root": "src/tests",
            "extraPaths": [
                "src/tests/e2e",
                "src/sdk"
            ]
        },
        {
            "root": "src"
        }
    ]
}